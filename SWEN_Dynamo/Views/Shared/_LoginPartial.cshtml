@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
    @Html.AntiForgeryToken()

    <ul class="nav navbar-nav navbar-right">
        <li>
            @Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })
        </li>
        <li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>
    </ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">

        <li><a href="http://societyofwomenengineers.swe.org/privacy-policy">SWE Privacy Policy</a></li>
        <li>@Html.ActionLink("Take Survey", "TakeSurvey", "Survey")</li>
        <li>@Html.ActionLink("SWE Member Log in", "Index", "Login"@*, routeValues: null, htmlAttributes: new { id = "loginLink" }*@)</li>
    </ul>
}
